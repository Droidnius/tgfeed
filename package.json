{
  "name": "tgfeed",
  "version": "1.0.0",
  "description": "Telegram feed",
  "author": "Alexander Shershnev",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh-a-v/tgfeed.git"
  },
  "bugs": {
    "url": "https://github.com/sh-a-v/tgfeed/issues"
  },
  "homepage": "https://github.com/sh-a-v/tgfeed#readme",
  "scripts": {
    "dev": "npm run clean && BUILD_ENV=dev NODE_ENV=development BABEL_ENV=development webpack serve --progress",
    "serve": "npm run clean && BUILD_ENV=dev NODE_ENV=production BABEL_ENV=production webpack serve --progress",
    "build": "BUILD_ENV=prod NODE_ENV=production BABEL_ENV=production webpack && npm run inject-sw-manifest",
    "clean": "rimraf build",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint-fix": "eslint src --fix --ext .js,.ts,.tsx",
    "analyze-bundle": "npm run clean && BUNDLE_ANALYZER=true BUILD_ENV=dev NODE_ENV=production BABEL_ENV=production webpack",
    "check-es5": "npx are-you-es5 check ./",
    "generate-pwa-images": "npx pwa-asset-generator ./src/ui/icons/app.svg ./src/ui/images --background '#0082C8' --opaque false --type png --splash-only",
    "inject-sw-manifest": "workbox injectManifest workbox.config.js"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "comlink": "4.3.1",
    "idb-keyval": "6.2.0",
    "solid-js": "1.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@sentry/webpack-plugin": "1.18.9",
    "@types/classnames": "2.3.0",
    "@types/node": "18.0.0",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "babel-preset-solid": "1.4.5",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "html-inline-css-webpack-plugin": "1.11.1",
    "html-loader": "3.1.2",
    "html-webpack-plugin": "5.5.0",
    "html-webpack-skip-assets-plugin": "1.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.0",
    "rimraf": "3.0.2",
    "solid-refresh": "0.4.1",
    "style-loader": "3.3.1",
    "stylus": "0.58.1",
    "stylus-loader": "7.0.0",
    "terser-webpack-plugin": "5.3.3",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3",
    "webpack-webmanifest-loader": "2.0.2",
    "workbox-cli": "6.5.3"
  },
  "sideEffects": false
}
