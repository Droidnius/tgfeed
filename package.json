{
  "name": "tgfeed",
  "version": "1.0.0",
  "description": "Telegram Feed",
  "author": "Aleksandr Shershnev",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh-a-v/tgfeed.git"
  },
  "bugs": {
    "url": "https://github.com/sh-a-v/tgfeed/issues"
  },
  "homepage": "https://github.com/sh-a-v/tgfeed#readme",
  "scripts": {
    "dev": "npm run clean && NODE_ENV=development webpack serve --progress",
    "serve": "npm run clean && NODE_ENV=production webpack serve --progress",
    "build": "NODE_ENV=production webpack",
    "_build": "NODE_ENV=production webpack && npm run inject-sw-manifest",
    "clean": "rimraf build",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint-fix": "eslint src --fix --ext .js,.ts,.tsx",
    "analyze": "npm run clean && BUNDLE_ANALYZER=true NODE_ENV=production webpack",
    "check-es5": "npx are-you-es5 check ./",
    "generate-pwa-images": "npx pwa-asset-generator ./src/shared/icons/app.svg ./src/shared/images --bg '#0082C8' --opaque false --type png --splash-only",
    "inject-sw-manifest": "workbox injectManifest workbox.config.js"
  },
  "engines": {
    "node": ">=19.8.0",
    "npm": ">=9.5.0"
  },
  "sideEffects": false,
  "dependencies": {
    "@cryptography/aes": "0.1.1",
    "@cryptography/pbkdf2": "0.1.2",
    "@cryptography/sha1": "0.2.0",
    "@cryptography/sha256": "0.2.0",
    "@cryptography/sha512": "0.2.0",
    "@floating-ui/dom": "1.2.9",
    "clsx": "1.2.1",
    "comlink": "4.4.1",
    "compression-streams-polyfill": "0.1.4",
    "core-js": "3.30.2",
    "idb": "7.1.1",
    "solid-floating-ui": "0.2.1",
    "solid-js": "1.7.5"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/preset-env": "7.22.4",
    "@babel/preset-typescript": "7.21.5",
    "@csstools/postcss-global-data": "1.0.3",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "babel-loader": "9.1.2",
    "babel-preset-solid": "1.7.4",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.41.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-solid": "0.12.1",
    "glob": "10.2.6",
    "html-inline-css-webpack-plugin": "1.11.1",
    "html-inline-script-webpack-plugin": "3.1.0",
    "html-webpack-plugin": "5.5.1",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.24",
    "postcss-custom-media": "9.1.3",
    "postcss-dark-theme-class": "0.7.3",
    "postcss-easy-import": "4.0.0",
    "postcss-functions": "4.0.2",
    "postcss-loader": "7.3.2",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "6.0.1",
    "postcss-preset-env": "8.4.1",
    "postcss-simple-vars": "7.0.1",
    "postcss-variable-compress": "3.0.0",
    "rimraf": "5.0.1",
    "solid-refresh": "0.5.2",
    "style-loader": "3.3.3",
    "stylelint": "15.6.2",
    "stylelint-config-recommended": "12.0.0",
    "sugarss": "4.0.1",
    "svg-symbol-sprite-loader": "5.1.0",
    "terser-webpack-plugin": "5.3.9",
    "typescript": "5.0.4",
