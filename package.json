{
  "name": "tgfeed",
  "version": "1.0.0",
  "description": "Telegram Feed",
  "author": "Aleksandr Shershnev",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh-a-v/tgfeed.git"
  },
  "bugs": {
    "url": "https://github.com/sh-a-v/tgfeed/issues"
  },
  "homepage": "https://github.com/sh-a-v/tgfeed#readme",
  "scripts": {
    "dev": "npm run clean && ENV_VARS=dev NODE_ENV=development webpack serve --progress",
    "serve": "npm run clean && ENV_VARS=dev NODE_ENV=production webpack serve --progress",
    "build": "ENV_VARS=prod NODE_ENV=production webpack && npm run inject-sw-manifest",
    "clean": "rimraf build",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint-fix": "eslint src --fix --ext .js,.ts,.tsx",
    "analyze-bundle": "npm run clean && BUNDLE_ANALYZER=true ENV_VARS=dev NODE_ENV=production webpack",
    "check-es5": "npx are-you-es5 check ./",
    "generate-pwa-images": "npx pwa-asset-generator ./src/ui/icons/app.svg ./src/ui/images --bg '#0082C8' --opaque false --type png --splash-only",
    "inject-sw-manifest": "workbox injectManifest workbox.config.js"
  },
  "dependencies": {
    "@cryptography/aes": "0.1.1",
    "@cryptography/pbkdf2": "0.1.2",
    "@cryptography/sha1": "0.2.0",
    "@cryptography/sha256": "0.2.0",
    "@cryptography/sha512": "0.2.0",
    "@sentry/browser": "7.41.0",
    "clsx": "1.2.1",
    "comlink": "4.4.1",
    "core-js": "3.29.0",
    "idb": "7.1.1",
    "pako": "2.1.0",
    "solid-js": "1.6.12"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@csstools/postcss-global-data": "1.0.3",
    "@sentry/webpack-plugin": "1.20.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "babel-loader": "9.1.2",
    "babel-preset-solid": "1.6.12",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-solid": "0.11.0",
    "glob": "9.2.1",
    "html-inline-css-webpack-plugin": "1.11.1",
    "html-inline-script-webpack-plugin": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.7.3",
    "postcss": "8.4.21",
    "postcss-custom-media": "9.1.2",
    "postcss-dark-theme-class": "0.7.3",
    "postcss-easy-import": "4.0.0",
    "postcss-functions": "4.0.2",
    "postcss-loader": "7.0.2",
